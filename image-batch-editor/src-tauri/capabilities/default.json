{
  "$schema": "https://schema.tauri.app/config/2.0.0",
  "identifier": "default",
  "description": "Default permissions for the application",
  "windows": ["main"],
  "permissions": [
    "core:default",
    "core:window:default",
    "core:event:default",
    "core:event:allow-listen",
    "core:path:default",
    "core:app:default",
    "dialog:default",
    "fs:default",
    "fs:allow-read-file",
    "fs:allow-read-dir",
    "fs:allow-write-file",
    "fs:allow-exists",
    "fs:allow-mkdir",
    "fs:allow-create",
    "shell:default"
  ]
}
